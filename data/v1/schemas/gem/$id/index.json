{
  "$defs": {
    "ResourceRef_GemCategory_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_Gem_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "SkillEffectInUse": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/skill_effect_in_use/"
    }
  },
  "properties": {
    "next_level_gem": {
      "anyOf": [
        {
          "$ref": "#/$defs/ResourceRef_Gem_"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "该宝石的下一等级的引用，当为None时表示该宝石为最高等级"
    },
    "category": {
      "$ref": "#/$defs/ResourceRef_GemCategory_",
      "description": "宝石类型引用"
    },
    "effect": {
      "description": "宝石效果",
      "items": {
        "$ref": "#/$defs/SkillEffectInUse"
      },
      "title": "Effect",
      "type": "array"
    },
    "id": {
      "description": "资源ID",
      "title": "Id",
      "type": "integer"
    },
    "name": {
      "description": "宝石名称",
      "title": "Name",
      "type": "string"
    },
    "level": {
      "description": "宝石等级",
      "title": "Level",
      "type": "integer"
    },
    "generation_id": {
      "description": "宝石世代",
      "title": "Generation Id",
      "type": "integer"
    },
    "upgrade_cost": {
      "description": "升级到该等级需要的石之砂数量",
      "title": "Upgrade Cost",
      "type": "integer"
    },
    "hash": {
      "description": "数据哈希值，目前pydantic不支持对key进行排序，所以哈希值变化可能意味着数据结构变化而非数据值变化",
      "title": "Hash",
      "type": "string"
    }
  },
  "required": [
    "category",
    "effect",
    "id",
    "name",
    "level",
    "generation_id",
    "upgrade_cost",
    "hash"
  ],
  "title": "GemGen2",
  "type": "object",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}