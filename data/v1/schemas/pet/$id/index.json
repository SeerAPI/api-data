{
  "$defs": {
    "DiyStats": {
      "properties": {
        "min": {
          "$ref": "#/$defs/SixAttributes",
          "description": "精灵自定义种族值最小值"
        },
        "max": {
          "$ref": "#/$defs/SixAttributes",
          "description": "精灵自定义种族值最大值"
        }
      },
      "required": [
        "min",
        "max"
      ],
      "title": "DiyStats",
      "type": "object"
    },
    "ResourceRef_PetArchiveStoryEntry_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_PetClass_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_PetEncyclopediaEntry_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_PetGenderCategory_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_PetMountTypeCategory_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_PetVipBuffCategory_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_SkillActivationItem_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_Skill_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_Soulmark_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "ResourceRef_TypeCombination_": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/resource_ref/"
    },
    "SixAttributes": {
      "$ref": "https://api.seerapi.com/v1/schemas/common/six_attributes/"
    },
    "SkillInPet": {
      "properties": {
        "learning_level": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "技能的学习等级，当该技能无法通过升级获得时，该字段为null",
          "title": "Learning Level"
        },
        "is_special": {
          "default": false,
          "description": "是否是特训技能",
          "title": "Is Special",
          "type": "boolean"
        },
        "is_advanced": {
          "default": false,
          "description": "是否是神谕觉醒技能",
          "title": "Is Advanced",
          "type": "boolean"
        },
        "is_fifth": {
          "default": false,
          "description": "是否是第五技能",
          "title": "Is Fifth",
          "type": "boolean"
        },
        "skill": {
          "$ref": "#/$defs/ResourceRef_Skill_",
          "description": "技能资源"
        },
        "skill_activation_item": {
          "anyOf": [
            {
              "$ref": "#/$defs/ResourceRef_SkillActivationItem_"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "学习该技能需要的激活道具"
        }
      },
      "required": [
        "skill"
      ],
      "title": "SkillInPet",
      "type": "object"
    }
  },
  "properties": {
    "id": {
      "description": "资源ID",
      "title": "Id",
      "type": "integer"
    },
    "name": {
      "description": "精灵名称",
      "title": "Name",
      "type": "string"
    },
    "yielding_exp": {
      "description": "击败精灵可获得的经验值",
      "title": "Yielding Exp",
      "type": "integer"
    },
    "catch_rate": {
      "description": "精灵捕捉率",
      "title": "Catch Rate",
      "type": "integer"
    },
    "evolving_lv": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵进化等级，当精灵无法再通过等级进化时为null",
      "title": "Evolving Lv"
    },
    "releaseable": {
      "description": "精灵是否可放生",
      "title": "Releaseable",
      "type": "boolean"
    },
    "fusion_master": {
      "description": "精灵是否可作为融合精灵的主宠",
      "title": "Fusion Master",
      "type": "boolean"
    },
    "fusion_sub": {
      "description": "精灵是否可作为融合精灵的副宠",
      "title": "Fusion Sub",
      "type": "boolean"
    },
    "has_resistance": {
      "description": "精灵是否可获得抗性",
      "title": "Has Resistance",
      "type": "boolean"
    },
    "resource_id": {
      "description": "精灵资源ID",
      "title": "Resource Id",
      "type": "integer"
    },
    "enemy_resource_id": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "该精灵在对手侧时使用的资源的ID，仅少数精灵存在这种资源",
      "title": "Enemy Resource Id"
    },
    "type": {
      "$ref": "#/$defs/ResourceRef_TypeCombination_",
      "description": "精灵属性"
    },
    "gender": {
      "$ref": "#/$defs/ResourceRef_PetGenderCategory_",
      "description": "精灵性别"
    },
    "base_stats": {
      "$ref": "#/$defs/SixAttributes",
      "description": "精灵种族值"
    },
    "pet_class": {
      "anyOf": [
        {
          "$ref": "#/$defs/ResourceRef_PetClass_"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵类别，同一进化链中的精灵属于同一类"
    },
    "evolution_chain_index": {
      "description": "该精灵在进化链中的位置，从0开始",
      "title": "Evolution Chain Index",
      "type": "integer"
    },
    "yielding_ev": {
      "$ref": "#/$defs/SixAttributes",
      "description": "击败精灵可获得的学习力"
    },
    "vipbuff": {
      "anyOf": [
        {
          "$ref": "#/$defs/ResourceRef_PetVipBuffCategory_"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵VIP加成，仅在闪光/暗黑精灵上使用"
    },
    "mount_type": {
      "anyOf": [
        {
          "$ref": "#/$defs/ResourceRef_PetMountTypeCategory_"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵骑乘类型，仅在坐骑精灵上使用"
    },
    "diy_stats": {
      "anyOf": [
        {
          "$ref": "#/$defs/DiyStats"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵自定义种族值，仅在合体精灵王上使用"
    },
    "skill": {
      "description": "精灵可学习的技能",
      "items": {
        "$ref": "#/$defs/SkillInPet"
      },
      "title": "Skill",
      "type": "array"
    },
    "soulmark": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/ResourceRef_Soulmark_"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵可持有的魂印",
      "title": "Soulmark"
    },
    "encyclopedia_entry": {
      "anyOf": [
        {
          "$ref": "#/$defs/ResourceRef_PetEncyclopediaEntry_"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵图鉴条目"
    },
    "archive_story_entry": {
      "anyOf": [
        {
          "$ref": "#/$defs/ResourceRef_PetArchiveStoryEntry_"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "精灵故事条目"
    },
    "hash": {
      "description": "数据哈希值，目前pydantic不支持对key进行排序，所以哈希值变化可能意味着数据结构变化而非数据值变化",
      "title": "Hash",
      "type": "string"
    }
  },
  "required": [
    "id",
    "name",
    "yielding_exp",
    "catch_rate",
    "releaseable",
    "fusion_master",
    "fusion_sub",
    "has_resistance",
    "resource_id",
    "type",
    "gender",
    "base_stats",
    "evolution_chain_index",
    "yielding_ev",
    "skill",
    "hash"
  ],
  "title": "Pet",
  "type": "object",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}