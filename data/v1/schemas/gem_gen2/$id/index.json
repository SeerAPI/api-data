        },
        "id": {
          "description": "宝石ID",
          "title": "Id",
          "type": "integer"
        },
        "name": {
          "description": "宝石名称",
          "title": "Name",
          "type": "string"
        },
        "level": {
          "description": "宝石等级",
          "title": "Level",
          "type": "integer"
        },
        "generation_id": {
          "description": "宝石世代",
          "title": "Generation Id",
          "type": "integer"
        },
        "upgrade_cost": {
          "description": "升级到该等级需要的石之砂数量",
          "title": "Upgrade Cost",
          "type": "integer"
        }
      },
      "required": [
        "category",
        "effect",
        "item",
        "id",
        "name",
        "level",
        "generation_id",
        "upgrade_cost"
