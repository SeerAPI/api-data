    }
  },
  "properties": {
    "id": {
      "description": "资源ID",
      "title": "Id",
      "type": "integer"
    },
    "name": {
      "description": "部件类型名称",
      "title": "Name",
      "type": "string"
    },
    "equip": {
      "description": "部件列表",
